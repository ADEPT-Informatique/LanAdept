@model IEnumerable<LanAdeptData.Model.Reservation>

<h4>Résultats</h4>
<table class="table">
	<thead>
		<tr>
			<th>Nom complet</th>
			<th>Email</th>
		</tr>
	</thead>
	<tbody>

		@foreach (LanAdeptData.Model.Reservation reservation in Model)
		{
			<tr class="vertical-align clickable-row" data-row-url="@Url.Action("Details", new { id = reservation.Place.SeatsId })">
				<td>@reservation.UserCompleteName</td>
				<td>@(reservation.IsGuest ? "--" : reservation.User.Email)</td>
                <td>@reservation.Place.SeatsId</td>
			</tr>
		}
	</tbody>
</table>